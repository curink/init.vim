{
  "coc.source.gocode.enable": false,
  "languageserver": {
    "go": {
      "command": "go-langserver",
      "filetypes": ["go"],
      "cwd": ".",
      "initializationOptions": {
        "gocodeCompletionEnabled": true,
        "diagnosticsEnabled": true,
        "lintTool": "golint"
      }
    },
    "bash": {
      "command": "bash-language-server",
      "filetypes": ["sh"],
      "args": ["start"]
    }
  }
}
