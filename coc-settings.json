{
  "languageserver": {
    "go": {
      "command": "go-langserver",
      "filetypes": ["go"],
      "cwd": ".",
      "revealOutputChannelOn": "never",
      "initializationOptions": {
        "gocodeCompletionEnabled": true,
        "diagnosticsEnabled": true,
        "lintTool": "golint"
      }
    },
    "bash": {
      "command": "bash-language-server",
      "filetypes": ["sh"],
      "args": ["start"]
    }
  }
}
